@import "../../../common/css/variable.styl"

.home-notice-wrap
  position relative
  width 14.4rem
  padding-top .7rem
  background $normal-white-color

  .home-notice-content
    position absolute
    left 50%
    transform translateX(-50%)
    height .22rem
    overflow hidden
    p
      float left
      margin-right .2rem
    ul
      float left
      li
        height .22rem

